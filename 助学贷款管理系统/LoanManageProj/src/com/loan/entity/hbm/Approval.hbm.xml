<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.loan.entity.Approval" table="ApprovalInfo" schema="dbo" catalog="LoanDB">
		<id name="approvalId" type="java.lang.Integer"> 
		    <column name="ApprovalId" />
		    <generator class="native" />
		</id>
		<property name="approvalStatu" type="java.lang.String">
			<column name="ApprovalStatu"/>
		</property>
		
		
		<set name="apply" inverse="true">
            <key>
                <column name="ApprovalId" not-null="false" />
            </key>
            <one-to-many class="com.loan.entity.Apply" />
        </set>
		
	</class>
</hibernate-mapping>
